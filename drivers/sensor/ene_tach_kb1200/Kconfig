config TACH_ENE_KB1200
	bool "ENE KB1200 tachometer sensor"
	default y
	depends on DT_HAS_ENE_KB1200_TACH_ENABLED
	select PINCTRL
	help
	  Enable tachometer sensor for ENE KB1200.
