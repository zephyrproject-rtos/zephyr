# RRH46410 air quality sensor

# Copyright (c) 2024 Renesas Electronics Corporation
# SPDX-License-Identifier: Apache-2.0

config RRH46410
	bool "RRH46410 sensor"
	default y
	depends on DT_HAS_RENESAS_RRH46410_ENABLED
	select I2C if $(dt_compat_on_bus,$(DT_COMPAT_RENESAS_RRH46410),i2c)
	select UART if $(dt_compat_on_bus,$(DT_COMPAT_RENESAS_RRH46410),uart)
	depends on GPIO

	help
	  Enable driver for RRH46410 TVOC sensor.
