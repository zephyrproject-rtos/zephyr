config UART_ENE_KB1200
	bool "ENE KB1200 serial driver"
	default y
	depends on DT_HAS_ENE_KB1200_UART_ENABLED
	select SERIAL_HAS_DRIVER
	select SERIAL_SUPPORT_INTERRUPT
	select PINCTRL
	help
	  This option enables the KB1200 serial driver.
