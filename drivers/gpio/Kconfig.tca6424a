menuconfig GPIO_TCA6424A
	bool "TCA6424A driver"
	default y
	depends on DT_HAS_TI_TCA6424A_ENABLED
	depends on I2C
	help
	  Enable driver for TCA6424A IO expander chip.

config GPIO_TCA6424A_INIT_PRIORITY
	int "Init priority"
	default I2C_INIT_PRIORITY
