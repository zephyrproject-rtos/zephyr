config TIMEAWARE_GPIO_INTEL
	bool "Intel Time aware GPIO driver"
	default y
	depends on DT_HAS_INTEL_TIMEAWARE_GPIO_ENABLED
	help
	  Intel time aware GPIO driver found on Intel PCH subsystem
