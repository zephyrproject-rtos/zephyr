# Renesas RX External Interrupt Option

config RENESAS_RX_EXTERNAL_INTERRUPT
	bool "Renesas RX External Interrupt Driver"
	depends on DT_HAS_RENESAS_RX_EXTERNAL_INTERRUPT_ENABLED
	default y
	help
	  Enable config options for Renesas RX external interrupt
