config REGULATOR_MSPM0_VREF
	bool "TI MSPM0 VREF peripheral driver"
	depends on REGULATOR
	depends on DT_HAS_TI_MSPM0_VREF_ENABLED
	default y
	select USE_MSPM0_DL_VREF
	help
	  Enable the MSPM0 VREF driver

config REGULATOR_MSPM0_VREF_INIT_PRIORITY
	int "MSPM0 VREF peripheral driver init priority"
	default 45
	depends on REGULATOR_MSPM0_VREF
	help
	  Init priority for the MSPM0 VREF peripheral.
