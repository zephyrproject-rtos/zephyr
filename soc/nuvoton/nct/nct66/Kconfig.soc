config SOC_SERIES_NCT66
	bool
	select SOC_FAMILY_NCT
	help
	  Enable support for Nuvoton NCT6 series

config SOC_NCT6694B
	bool
	select SOC_SERIES_NCT66
	help
	  NCT6694B

config SOC_SERIES
	default "nct66" if SOC_SERIES_NCT66

config SOC
	default "nct6694b" if SOC_NCT6694B
