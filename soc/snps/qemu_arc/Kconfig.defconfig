if SOC_QEMU_ARC

config SYS_CLOCK_HW_CYCLES_PER_SEC
	int
	default 10000000

config RGF_NUM_BANKS
	default 1

config ARC_FIRQ
	default n

config NUM_IRQ_PRIO_LEVELS
	default 15

config NUM_IRQS
	default 26

