config SOC_FAMILY_INTEL_ISH
	select X86
	select CPU_LAKEMONT_VALUE
	select X86_NO_SPECULATIVE_VULNERABILITIES
	select IOAPIC
	select LOAPIC
	select INTEL_HAL
	select HAS_PM
	select HAS_COVERAGE_SUPPORT
	select SOC_EARLY_INIT_HOOK
