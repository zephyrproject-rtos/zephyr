config SOC_VERSAL_RPU
	select ARM
	select CPU_CORTEX_R5
	select CPU_HAS_ARM_MPU
	select CPU_HAS_DCLS
	select VFP_DP_D16
	select SOC_RESET_HOOK

config SOC_VERSAL_APU
	select ARM64
	select ARM_ARCH_TIMER
	select CPU_CORTEX_A72
	select CPU_HAS_MMU
