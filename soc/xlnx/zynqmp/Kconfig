config SOC_XILINX_ZYNQMP_RPU
	select ARM
	select CPU_CORTEX_R5
	select SOC_RESET_HOOK
	select SOC_EARLY_INIT_HOOK
	select CPU_HAS_ARM_MPU
	select ARM_MPU
