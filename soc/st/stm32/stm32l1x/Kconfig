config SOC_SERIES_STM32L1X
	select ARM
	select CPU_CORTEX_M3
	select CPU_CORTEX_M_HAS_DWT
	select CPU_HAS_ARM_MPU
	select HAS_PM
	select HAS_POWEROFF
	select HAS_STM32CUBE
	select HAS_SWO
	select SOC_EARLY_INIT_HOOK
