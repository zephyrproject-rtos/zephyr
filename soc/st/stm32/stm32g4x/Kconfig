config SOC_SERIES_STM32G4X
	select ARM
	select CPU_CORTEX_M4
	select CPU_CORTEX_M_HAS_DWT
	select CPU_HAS_FPU
	select HAS_STM32CUBE
	select HAS_STM32_FLASH_PREFETCH
	select CPU_HAS_ARM_MPU
	select HAS_PM
	select HAS_SWO
	select SOC_EARLY_INIT_HOOK
