choice
prompt "STM32F3x MCU Selection"
depends on SOC_SERIES_STM32F3X

config SOC_STM32F302X8
	bool "STM32F302X8"

config SOC_STM32F303XC
	bool "STM32F303XC"
	select CPU_HAS_ARM_MPU

config SOC_STM32F334X8
	bool "STM32F334X8"

config SOC_STM32F373XC
	bool "STM32F373XC"
	select CPU_HAS_ARM_MPU

endchoice
