if SOC_LPC54114_M4

config SOC
	default "lpc54114"

if PINMUX

config PINMUX_MCUX_LPC
	default y

endif # PINMUX

if GPIO

config GPIO_MCUX_LPC
	default y

endif # GPIO

if SERIAL

config USART_MCUX_LPC
	default y

endif # SERIAL

