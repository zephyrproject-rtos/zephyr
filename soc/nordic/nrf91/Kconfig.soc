config SOC_SERIES_NRF91X
	select SOC_FAMILY_NORDIC_NRF
	help
	  Enable support for NRF91 MCU series

config SOC_NRF9120
	bool
	select SOC_SERIES_NRF91X

config SOC_NRF9160
	bool
	select SOC_SERIES_NRF91X

config SOC_NRF9160_SICA
	bool
	select SOC_NRF9160

config SOC
	default "nrf9160" if SOC_NRF9160_SICA
	default "nrf9120" if SOC_NRF9120
