config SOC_SERIES_RP2350
	bool
	select SOC_FAMILY_RPI_PICO

config SOC_SERIES
	default "rp2350" if SOC_SERIES_RP2350

config SOC_RP2350A
	bool
	select SOC_SERIES_RP2350

config SOC
	default "rp2350a" if SOC_RP2350A
