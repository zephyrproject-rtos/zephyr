config SOC_SERIES_KE1XZ
	select ARM
	select CPU_CORTEX_M0PLUS
	select CPU_CORTEX_M_HAS_SYSTICK
	select CPU_CORTEX_M_HAS_VTOR
	select CLOCK_CONTROL
	select HAS_MCUX
	select HAS_PM
	select SOC_RESET_HOOK
