config SOC_SERIES_RA8M1
	select ARM
	select CPU_CORTEX_M85
	select CPU_HAS_ARM_MPU
	select CPU_HAS_FPU
	select CPU_HAS_ARM_SAU
	select ARMV8_M_DSP
	select FPU
	select HAS_SWO
	select XIP
	select HAS_RENESAS_RA_FSP
	help
	  Enable support for Renesas RA8M1 MCU series
