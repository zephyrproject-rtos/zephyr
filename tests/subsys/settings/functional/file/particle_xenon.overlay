/delete-node/ &storage_partition;

&mx25l32 {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		littlefs_partition: partition@0 {
			label = "littlefs";
			reg = <0x00000000 0x00200000>;
		};

		storage_partition: partition@200000 {
			label = "storage";
			reg = <0x00200000 0x00004000>;
		};

		partition@220000 {
			label = "scratch2";
			reg = <0x00204000 0x001fc000>;
		};
	};
};
