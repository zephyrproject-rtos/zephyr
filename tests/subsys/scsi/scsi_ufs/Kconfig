mainmenu "SCSI UFS test"

config TEST_SCSI_UFS_RW
	bool "SCSI UFS RW Tests (DANGEROUS)"
	help
	  To perform Writing to UFS device as part of SCSI RW Tests.
	  This will overwrite existing data on UFS device.

source "Kconfig.zephyr"
