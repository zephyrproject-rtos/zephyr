&spi5 {
	dmas = <&gpdma1 0 88 STM32_DMA_PERIPH_TX>,
	       <&gpdma1 1 87 STM32_DMA_PERIPH_RX>;
	dma-names = "tx", "rx";
	slow@0 {
		compatible = "test-spi-loopback-slow";
		reg = <0>;
		spi-max-frequency = <500000>;
	};
	fast@0 {
		compatible = "test-spi-loopback-fast";
		reg = <0>;
		spi-max-frequency = <16000000>;
	};
};

&gpdma1 {
	status = "okay";
};
