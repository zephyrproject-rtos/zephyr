#include "test_common.dtsi"

/ {
	regulator {
		/* Arduino D0 */
		enable-gpios = <&gpio1 1 GPIO_ACTIVE_HIGH>;
	};

	resources {
		/* Arduino D1 */
		check-gpios = <&gpio1 2 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>;
	};
