/delete-node/ &storage_partition;

&flash0 {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		/* Set 2 last blocks of the code flash as storage partition */
		storage_partition: partition@1F0000 {
			label = "storage";
			reg = <0x1F0000 DT_SIZE_K(64)>;
		};
	};
};
