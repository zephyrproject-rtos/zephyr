&code_mram_cm33 {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		storage_partition: partition@38000 {
			label = "storage";
			reg = <0x38000 DT_SIZE_K(32)>;
		};
	};
};
