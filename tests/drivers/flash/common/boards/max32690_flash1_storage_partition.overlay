/ {
	chosen {
		zephyr,flash-controller = &flc1;
		zephyr,flash = &flash1;
	};
};

&flash1 {
	status = "okay";

	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		storage_partition: partition@0 {
			label = "storage";
			reg = <0x0 DT_SIZE_K(256)>;
		};
	};
};
