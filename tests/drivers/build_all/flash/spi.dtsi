at45@0 {
	compatible = "atmel,at45";
	reg = <0x0>;
	status = "okay";
	spi-max-frequency = <5000000>;
	jedec-id = [00 11 22];
	size = <1048576>;
	sector-size = <1>;
	sector-0a-pages = <1>;
	block-size = <1>;
	page-size = <1>;
};

spi-nor@1 {
	compatible = "jedec,spi-nor";
	reg = <0x1>;
	status = "okay";
	spi-max-frequency = <5000000>;
	size = <1048576>;
	jedec-id = [00 11 22];
};

at25xv021a@2 {
	compatible = "atmel,at25xv021a";
	reg = <0x2>;
	status = "okay";
	spi-max-frequency = <5000000>;
	jedec-id = [00 11 22];
	size = <262144>;
	page-size = <1>;
	timeout = <1>;
	timeout-erase = <1>;
};
