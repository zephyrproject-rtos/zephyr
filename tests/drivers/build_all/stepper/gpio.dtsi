/****************************************
 *  PLEASE KEEP ALPHABETICALLY SORTED   *
 ***************************************/

adi_tmc2209: adi_tmc2209 {
	compatible = "adi,tmc2209";
	status = "okay";
	micro-step-res = <1>;
	msx-gpios = <&test_gpio 0 0>,
		    <&test_gpio 0 0>;
	en-gpios = <&test_gpio 0 0>;
	step-gpios = <&test_gpio 0 0>;
	dir-gpios = <&test_gpio 0 0>;
	counter = <&counter0>;
};

ti_drv8424: ti_drv8424 {
	status = "okay";
	compatible = "ti,drv8424";

	dir-gpios = <&test_gpio 0 0>;
	step-gpios = <&test_gpio 0 0>;
	sleep-gpios = <&test_gpio 0 0>;
	en-gpios  = <&test_gpio 0 0>;
	m0-gpios = <&test_gpio 0 0>;
	m1-gpios = <&test_gpio 0 0>;
	counter = <&counter0>;
};

zephyr_gpio_stepper: zephyr_gpio_stepper {
	compatible = "zephyr,gpio-stepper";
	status = "okay";
	micro-step-res = <1>;
	gpios = <&test_gpio 0 0>,
		<&test_gpio 0 0>,
		<&test_gpio 0 0>,
		<&test_gpio 0 0>;
};
