/ {
	test {
		#address-cells = <1>;
		#size-cells = <1>;

		test_gpio: gpio@deadbeef {
			compatible = "vnd,gpio";
			gpio-controller;
			reg = <0xdeadbeef 0x1000>;
			#gpio-cells = <0x2>;
		};

		test_i2c: i2c@11112222 {
			#address-cells = <1>;
			#size-cells = <0>;
			compatible = "vnd,i2c";
			reg = <0x11112222 0x1000>;
			clock-frequency = <100000>;

			test_am1805: am1805@0 {
				compatible = "ambiq,am1805";
				reg = <0x0>;
				am1805-gpios = <&test_gpio 0 0>;
			};

			test_pcf8523: pcf8523@1 {
				compatible = "nxp,pcf8523";
				reg = <0x1>;
				alarms-count = <1>;
				battery-switch-over = "standard";
				int1-gpios = <&test_gpio 0 0>;
			};

			test_pcf8563: pcf8563@2 {
				compatible = "nxp,pcf8563";
				reg = <0x2>;
			};

			test_rv3028: rv3028@3 {
				compatible = "microcrystal,rv3028";
				reg = <0x3>;
				clkout-frequency = <1>;
				backup-switch-mode = "disabled";
				trickle-resistor-ohms = <3000>;
				int-gpios = <&test_gpio 0 0>;
			};

			test_rv8263: rv8263@4 {
				compatible = "microcrystal,rv-8263-c8";
				reg = <0x4>;
				int-gpios = <&test_gpio 0 0>;
				clkout = <4096>;
			};

			test_ds1307: ds1307@5 {
				compatible = "maxim,ds1307";
				reg = <0x5>;
			};

			test_rv8803: rv8803@6 {
				compatible = "microcrystal,rv8803";
				reg = <0x6>;
				int-gpios = <&test_gpio 0 0>;
				clkout-frequency = <1024>;
			};

			test_bq32002: bq32002@7 {
				compatible = "ti,bq32002";
				reg = <0x7>;
				irq-frequency = <512>;
			};

			test_rx8130ce: rx8130ce-rtc@8 {
				compatible = "epson,rx8130ce-rtc";
				reg = <0x8>;
				irq-gpios = <&test_gpio 0 0>;
			};

			test_ds1337: ds1337@9 {
				compatible = "maxim,ds1337";
				reg = <0x9>;
				int-gpios = <&test_gpio 0 0>;
				sqw-frequency = <4096>;
			};

			test_ds3231: ds3231@a {
				compatible = "maxim,ds3231-mfd";
				reg = <0xa>;

				test_ds3231_rtc: ds3231_rtc {
					compatible = "maxim,ds3231-rtc";
					isw-gpios = <&test_gpio 0 0>;
					freq-32khz-gpios = <&test_gpio 0 0>;
				};
			};

			test_pcf85063a: pcf85063a@b {
				compatible = "nxp,pcf85063a";
				reg = <0xb>;
				int1-gpios = <&test_gpio 0 0>;
			};

			test_rv3032: rv3032c7@c {
				compatible = "microcrystal,rv3032-mfd";
				reg = <0xc>;
				int-gpios = <&test_gpio 0 0>;
				evi-gpios = <&test_gpio 0 0>;
				backup-switch-mode = "disabled";
				always-on;
				status = "okay";

				test_rv3032_rtc: rv3032_rtc {
					compatible = "microcrystal,rv3032";
					trickle-resistor-ohms = <600>;
					trickle-charger-mode = <1750>;
					status = "okay";
				};
			};
		};
	};
};
