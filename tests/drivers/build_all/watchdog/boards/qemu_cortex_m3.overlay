/ {
	watchdog: watchdog@88888888 {
		compatible = "xlnx,xps-timebase-wdt-1.00.a";
		reg = <0x88888888 0x10000>;
		clocks = <&sysclk>;
		xlnx,wdt-interval = <31>;
		xlnx,wdt-enable-once = <1>;
	};
};
