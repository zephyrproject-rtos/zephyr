&agt0 {
	interrupts = <30 1>, <31 1>;
	interrupt-names = "agti", "agtcmai";
	renesas,count-source = "AGT_CLOCK_LOCO";
	renesas,prescaler = <0>;
	status = "okay";

	counter0: counter {
		status = "okay";
	};
};
