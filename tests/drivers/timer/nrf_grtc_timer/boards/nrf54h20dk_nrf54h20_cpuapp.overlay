&grtc {
	/delete-property/ child-owned-channels;
	interrupts = <109 2>;
};

test_timer: &timer131 {
	status = "okay";
	interrupts = <419 1>;
};

&timer130 {
	status = "okay";
	prescaler = <0>;
};

&timer132 {
	status = "okay";
	interrupts = <434 0>;
};

/ {
	chosen {
		zephyr,cpu-load-counter = &timer130;
	};

	busy-sim {
		compatible = "vnd,busy-sim";
		status = "okay";
		counter = <&timer132>;
	};
};
