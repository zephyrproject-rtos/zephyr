/*
 * This is a loopback setup for the mcxw72_evk
 * To test this sample, connect J1.5 <-> J2.1 and J1.7 <-> J2.3
 */
&lpi2c1 {
	status = "okay";
	eeprom0: eeprom@54 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x54>;
		size = <256>;
	};
};

&lpi2c0 {
	status = "okay";
	pinctrl-0 = <&pinmux_lpi2c0>;
	pinctrl-names = "default";
	eeprom1: eeprom@56 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x56>;
		size = <256>;
	};
};

&gpiob {
	status = "okay";
};
