/* To test this sample, connect J41.12 <-> J44.20 and J41.10 <-> J44.18 */

&lpi2c2 {
	status = "okay";

	eeprom0: eeprom@54 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x54>;
		size = <256>;
	};
};

&lpi2c3 {
	status = "okay";

	eeprom1: eeprom@56 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x56>;
		size = <256>;
	};
};
