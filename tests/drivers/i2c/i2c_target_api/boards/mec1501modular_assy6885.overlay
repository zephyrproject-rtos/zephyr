&i2c_smb_0 {
	eeprom0: eeprom@54 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x54>;
		address-width = <16>;
		size = <1024>;
	};
	eeprom2: eeprom@58 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x58>;
		size = <4096>;
		address-width = <16>;
		read-only;
	};
};


&i2c_smb_1 {
	eeprom1: eeprom@56 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x56>;
		address-width = <16>;
		size = <1024>;
	};
};
