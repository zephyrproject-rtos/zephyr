&i2c0 {
	eeprom0: eeprom@54 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x54>;
		address-width = <16>;
		size = <1024>;
	};
};

&i2c1 {
	status = "okay";
	pinctrl-0 = <&i2c1_scl_p0_12 &i2c1_sda_p0_13>;
	pinctrl-names = "default";

	eeprom1: eeprom@56 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x56>;
		address-width = <16>;
		size = <1024>;
	};
};
