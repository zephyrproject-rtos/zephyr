menuconfig SNTP
	bool "SNTP"
	select NET_SOCKETS
	help
	  Enable SNTP client library

if SNTP

module = SNTP
module-dep = NET_LOG
module-str = Log level for SNTP
module-help = Enable debug message of SNTP client library.
source "subsys/net/Kconfig.template.log_config.net"

endif # SNTP
