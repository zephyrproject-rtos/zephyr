		gpion: gpio@f0160e {
			compatible = "ite,it51xxx-gpio";
			reg = <0x00f0160e 1   /* GPDR (set) */
			       0x00f0166e 1   /* GPDMR (get) */
			       0x00f0167e 1   /* GPOTR */
			       NO_FUNC 1      /* P18SCR */
			       0x00f016a8 8   /* GPCR */
			       0x00f01d2c 1>; /* KSIFSELR */
