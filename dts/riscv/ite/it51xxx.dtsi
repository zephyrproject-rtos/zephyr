		gpiom: gpio@f0160d {
			compatible = "ite,it51xxx-gpio";
			reg = <0x00f0160d 1   /* GPDR (set) */
			       0x00f0166d 1   /* GPDMR (get) */
			       NO_FUNC 1      /* GPOTR */
			       NO_FUNC 1      /* P18SCR */
			       0x00f016a0 7   /* GPCR */
			       NO_FUNC 1>;
			ngpios = <7>;
			gpio-controller;
			interrupts = <IT51XXX_IRQ_WU144 IRQ_TYPE_LEVEL_HIGH
