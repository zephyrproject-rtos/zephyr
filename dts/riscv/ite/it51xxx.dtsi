				      IT51XXX_IRQ_WU72 IRQ_TYPE_LEVEL_HIGH
				      IT51XXX_IRQ_WU73 IRQ_TYPE_LEVEL_HIGH
				      IT51XXX_IRQ_WU114 IRQ_TYPE_LEVEL_HIGH
				      IT51XXX_IRQ_WU40 IRQ_TYPE_LEVEL_HIGH
