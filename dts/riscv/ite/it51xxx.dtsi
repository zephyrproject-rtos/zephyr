#include <zephyr/dt-bindings/adc/adc.h>
#include <zephyr/dt-bindings/interrupt-controller/ite-it51xxx-intc.h>

/ {
	soc {
		adc0: adc@f01900 {
			compatible = "ite,it51xxx-adc";
			reg = <0xf01900 0x56>;
			interrupts = <8 IRQ_TYPE_LEVEL_HIGH>;
			interrupt-parent = <&intc>;
			status = "disabled";
			#io-channel-cells = <1>;
		};
	};
};
