	pinmux = <&iomuxc1_i2c1_scl_gpio_io_gpio1_io0>,
		<&iomuxc1_i2c1_sda_gpio_io_gpio1_io1>,
		<&iomuxc1_i2c2_scl_gpio_io_gpio1_io2>,
		<&iomuxc1_i2c2_sda_gpio_io_gpio1_io3>,
