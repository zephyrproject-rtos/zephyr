			interrupt-parent = <&{/interrupt-controller-unit/port_irq0}>;
			interrupts = <RA_ICU_EVENT_ICU_IRQ0>;
			reg = <0x40006000 0x1>;
			channel = <0>;
			renesas,sample-clock-div = <64>;
			#port-irq-cells = <0>;
			status = "disabled";
		};

