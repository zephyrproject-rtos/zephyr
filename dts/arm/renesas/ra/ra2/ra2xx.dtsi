		port_irq1 {
			#interrupt-cells = <1>;
			interrupt-map-mask = <0 0xfff>;
			interrupt-map = <
				0 RA_ICU_EVENT_ICU_IRQ1 &nvic 0 RA_NVIC_IRQN_UNSPECIFIED 1
			>;
		};

		port_irq2 {
			#interrupt-cells = <1>;
