				     <RA_ICU_EVENT_SPI0_TEI>,
				     <RA_ICU_EVENT_SPI0_ERI>;
			interrupt-names = "rxi", "txi", "tei", "eri";
			#address-cells = <1>;
			#size-cells = <0>;
			channel = <0>;
			reg = <0x40072000 0x100>;
			status = "disabled";
		};

