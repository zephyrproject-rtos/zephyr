			>;
		};

		agt1 {
			#interrupt-cells = <1>;
			interrupt-map-mask = <0 0xfff>;
			interrupt-map = <
				0 RA_ICU_EVENT_AGT1_INT &nvic 0 RA_NVIC_IRQN_UNSPECIFIED 1
				0 RA_ICU_EVENT_AGT1_COMPARE_A &nvic 0 RA_NVIC_IRQN_UNSPECIFIED 1
