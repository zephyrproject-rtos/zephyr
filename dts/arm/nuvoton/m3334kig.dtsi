#include <mem.h>
#include <nuvoton/m333x.dtsi>

/ {
	sram0: memory@20000000 {
		compatible = "mmio-sram";
		reg = <0x20000000 DT_SIZE_K(320)>;
	};

	soc {
		fmc: flash-controller@4000c000 {
			flash0: flash@0 {
				reg = <0 DT_SIZE_K(512)>;
			};
		};
	};
};
