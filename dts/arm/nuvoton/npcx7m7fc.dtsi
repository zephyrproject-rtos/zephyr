#include <mem.h>
#include "npcx7.dtsi"

/ {
	flash0: flash@10070000 {
		reg = <0x10070000 DT_SIZE_K(320)>;
	};

	flash1: flash@64000000 {
		reg = <0x64000000 DT_SIZE_K(512)>;
	};

	sram0: memory@200c0000 {
		compatible = "mmio-sram";
		reg = <0x200C0000 DT_SIZE_K(62)>;
	};
};
