#include <arm/armv7-m.dtsi>
#include <mem.h>
#include <zephyr/dt-bindings/gpio/gpio.h>
#include <zephyr/dt-bindings/i2c/i2c.h>
#include "m46x/sys.h"
#include "m46x/sys_reg.h"
#include "m46x/clk.h"
#include "m46x/clk_reg.h"

/ {
	chosen {
		zephyr,flash-controller = &fmc;
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-m4f";
			reg = <0>;
		};
	};

	sram0: memory@20000000 {
		compatible = "mmio-sram";
        reg = <0x20000000 DT_SIZE_K(512)>;
	};

	sysclk: system-clock {
		compatible = "fixed-clock";
		clock-frequency = <200000000>;
		#clock-cells = <0>;
	};
    
 
	soc {
		scc: system-clock-controller@40000200 {
			compatible = "nuvoton,numaker-scc";
			label = "SCC";
			reg = <0x40000200 0x100>;
			#clock-cells = <0>;
			/* hxt = "enable"; */
			/* lxt = "enable"; */
			clk-pclkdiv = <(CLK_PCLKDIV_APB0DIV_DIV2 | CLK_PCLKDIV_APB1DIV_DIV2)>;
			core-clock = <200000000>;

			pcc: peripheral-clock-controller {
				compatible = "nuvoton,numaker-pcc";
				label = "PCC";
				#clock-cells = <3>;
			};
		};

        fmc: flash-controller@4000c000 {
            compatible = "nuvoton,numaker-fmc";
            label = "FLASH_CTRL";
            reg = <0x4000c000 0x110>;
            #address-cells = <1>;
            #size-cells = <1>;

            flash0: flash@0 {
                compatible = "soc-nv-flash";
                label = "FLASH_NUMAKER";
                reg = <0 DT_SIZE_K(1024)>;
                erase-block-size = <4096>;
                write-block-size = <4>;
            };
        };

		uart0: serial@40070000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40070000 0x1000>;
			interrupts = <36 0>;
            reset = <UART0_RST>;
            clocks = <&pcc UART0_MODULE CLK_CLKSEL1_UART0SEL_HIRC CLK_CLKDIV0_UART0(1)>;
			status = "disabled";
			label = "UART_0";
		};

		uart1: serial@40071000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40071000 0x1000>;
			interrupts = <37 0>;
            reset = <UART1_RST>;
            clocks = <&pcc UART1_MODULE CLK_CLKSEL1_UART1SEL_HIRC CLK_CLKDIV0_UART1(1)>;
			status = "disabled";
			label = "UART_1";
		};

		uart2: serial@40072000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40072000 0x1000>;
			interrupts = <48 0>;
            reset = <UART2_RST>;
            clocks = <&pcc UART2_MODULE CLK_CLKSEL3_UART2SEL_HIRC CLK_CLKDIV4_UART2(1)>;
			status = "disabled";
			label = "UART_2";
		};

		uart3: serial@40073000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40073000 0x1000>;
			interrupts = <49 0>;
            reset = <UART3_RST>;
            clocks = <&pcc UART3_MODULE CLK_CLKSEL3_UART3SEL_HIRC CLK_CLKDIV4_UART3(1)>;
			status = "disabled";
			label = "UART_3";
		};

		uart4: serial@40074000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40074000 0x1000>;
			interrupts = <74 0>;
            reset = <UART4_RST>;
            clocks = <&pcc UART4_MODULE CLK_CLKSEL3_UART4SEL_HIRC CLK_CLKDIV4_UART4(1)>;
			status = "disabled";
			label = "UART_4";
		};

		uart5: serial@40075000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40075000 0x1000>;
			interrupts = <75 0>;
            reset = <UART5_RST>;
            clocks = <&pcc UART5_MODULE CLK_CLKSEL3_UART5SEL_HIRC CLK_CLKDIV4_UART5(1)>;
			status = "disabled";
			label = "UART_5";
		};

		uart6: serial@40076000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40076000 0x1000>;
			interrupts = <102 0>;
            reset = <UART6_RST>;
            clocks = <&pcc UART6_MODULE CLK_CLKSEL3_UART6SEL_HIRC CLK_CLKDIV4_UART6(1)>;
			status = "disabled";
			label = "UART_6";
		};

		uart7: serial@40077000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40077000 0x1000>;
			interrupts = <103 0>;
            reset = <UART7_RST>;
            clocks = <&pcc UART7_MODULE CLK_CLKSEL3_UART7SEL_HIRC CLK_CLKDIV4_UART7(1)>;
			status = "disabled";
			label = "UART_7";
		};
        
		uart8: serial@40078000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40078000 0x1000>;
			interrupts = <99 0>;
            reset = <UART8_RST>;
            clocks = <&pcc UART8_MODULE CLK_CLKSEL2_UART8SEL_HIRC CLK_CLKDIV5_UART8(1)>;
			status = "disabled";
			label = "UART_8";
		};
        
		uart9: serial@40079000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40079000 0x1000>;
			interrupts = <100 0>;
            reset = <UART9_RST>;
            clocks = <&pcc UART9_MODULE CLK_CLKSEL2_UART9SEL_HIRC CLK_CLKDIV5_UART9(1)>;
			status = "disabled";
			label = "UART_9";
		};        

		i2c0: i2c@40080000 {
			compatible = "nuvoton,numaker-i2c";
			clock-frequency = <I2C_BITRATE_STANDARD>;
			reg = <0x40080000 0x1000>;
			interrupts = <38 0>;
			reset = <I2C0_RST>;
			clocks = <&pcc I2C0_MODULE 0 0>;
			status = "disabled";
			label = "I2C_0";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c1: i2c@40081000 {
			compatible = "nuvoton,numaker-i2c";
			clock-frequency = <I2C_BITRATE_STANDARD>;
			reg = <0x40081000 0x1000>;
			interrupts = <39 0>;
			reset = <I2C1_RST>;
			clocks = <&pcc I2C1_MODULE 0 0>;
			status = "disabled";
			label = "I2C_1";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c2: i2c@40082000 {
			compatible = "nuvoton,numaker-i2c";
			clock-frequency = <I2C_BITRATE_STANDARD>;
			reg = <0x40082000 0x1000>;
			interrupts = <82 0>;
			reset = <I2C2_RST>;
			clocks = <&pcc I2C2_MODULE 0 0>;
			status = "disabled";
			label = "I2C_2";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c3: i2c@40083000 {
			compatible = "nuvoton,numaker-i2c";
			clock-frequency = <I2C_BITRATE_STANDARD>;
			reg = <0x40083000 0x1000>;
			interrupts = <83 0>;
			reset = <I2C3_RST>;
			clocks = <&pcc I2C3_MODULE 0 0>;
			status = "disabled";
			label = "I2C_3";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c4: i2c@40084000 {
			compatible = "nuvoton,numaker-i2c";
			clock-frequency = <I2C_BITRATE_STANDARD>;
			reg = <0x40084000 0x1000>;
			interrupts = <118 0>;
			reset = <I2C4_RST>;
			clocks = <&pcc I2C4_MODULE 0 0>;
			status = "disabled";
			label = "I2C_4";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		pinctrl: pin-controller@40000500 {
			compatible = "nuvoton,numaker-pinctrl";
			reg = <0x40000500 0xa0>;
			#address-cells = <1>;
			#size-cells = <1>;
			status = "okay";
			label = "PINCTRL";
		};
		gpioa: gpio@40004000 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004000 0x40>;
            clocks = <&pcc GPA_MODULE 0 0>;
			status = "disabled";
			interrupts = <16 2>;
			label = "GPIOA";
		};

		gpiob: gpio@40004040 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004040 0x40>;
            clocks = <&pcc GPB_MODULE 0 0>;
			status = "disabled";
			interrupts = <17 2>;
			label = "GPIOB";
		};

		gpioc: gpio@40004080 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004080 0x40>;
            clocks = <&pcc GPC_MODULE 0 0>;
			status = "disabled";
			interrupts = <18 2>;
			label = "GPIOC";
		};

		gpiod: gpio@400040c0 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x400040c0 0x40>;
            clocks = <&pcc GPD_MODULE 0 0>;
			status = "disabled";
			interrupts = <19 2>;
			label = "GPIOD";
		};

		gpioe: gpio@40004100 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004100 0x40>;
            clocks = <&pcc GPE_MODULE 0 0>;
			status = "disabled";
			interrupts = <20 2>;
			label = "GPIOE";
		};

		gpiof: gpio@40004140 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004140 0x40>;
            clocks = <&pcc GPF_MODULE 0 0>;
			status = "disabled";
			interrupts = <21 2>;
			label = "GPIOF";
		};

		gpiog: gpio@40004180 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004180 0x40>;
            clocks = <&pcc GPG_MODULE 0 0>;
			status = "disabled";
			interrupts = <72 2>;
			label = "GPIOG";
		};

		gpioh: gpio@400041c0 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x400041c0 0x40>;
            clocks = <&pcc GPH_MODULE 0 0>;
			status = "disabled";
			interrupts = <88 2>;
			label = "GPIOH";
		};

		gpioi: gpio@40004200 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004200 0x40>;
            clocks = <&pcc GPI_MODULE 0 0>;
			status = "disabled";
			interrupts = <110 2>;
			label = "GPIOI";
		};

		gpioj: gpio@40004240 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004240 0x40>;
            clocks = <&pcc GPJ_MODULE 0 0>;
			status = "disabled";
			interrupts = <61 2>;
			label = "GPIOJ";
		};

        spi0: spi@40061000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x40061000 0x6c>;
			interrupts = <23 0>;
            reset = <SPI0_RST>;
			label = "SPI_0";
			clocks = <&pcc SPI0_MODULE CLK_CLKSEL2_SPI0SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        spi1: spi@40062000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x40062000 0x6c>;
			interrupts = <51 0>;
            reset = <SPI1_RST>;
			label = "SPI_1";
			clocks = <&pcc SPI1_MODULE CLK_CLKSEL2_SPI1SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        spi2: spi@40063000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x40063000 0x6c>;
			interrupts = <52 0>;
            reset = <SPI2_RST>;
			label = "SPI_2";
			clocks = <&pcc SPI2_MODULE CLK_CLKSEL3_SPI2SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        spi3: spi@40064000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x40064000 0x6c>;
			interrupts = <62 0>;
            reset = <SPI3_RST>;
			label = "SPI_3";
			clocks = <&pcc SPI3_MODULE CLK_CLKSEL3_SPI3SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        spi4: spi@40065000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x40065000 0x6c>;
			interrupts = <63 0>;
            reset = <SPI4_RST>;
			label = "SPI_4";
			clocks = <&pcc SPI4_MODULE CLK_CLKSEL4_SPI4SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        spi5: spi@40066000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x40066000 0x6c>;
			interrupts = <57 0>;
            reset = <SPI5_RST>;
			label = "SPI_5";
			clocks = <&pcc SPI5_MODULE CLK_CLKSEL4_SPI5SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        spi6: spi@40067000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x40067000 0x6c>;
			interrupts = <70 0>;
            reset = <SPI6_RST>;
			label = "SPI_6";
			clocks = <&pcc SPI6_MODULE CLK_CLKSEL4_SPI6SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        spi7: spi@40068000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x40068000 0x6c>;
			interrupts = <77 0>;
            reset = <SPI7_RST>;
			label = "SPI_7";
			clocks = <&pcc SPI7_MODULE CLK_CLKSEL4_SPI7SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        spi8: spi@4006b000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x4006b000 0x6c>;
			interrupts = <108 0>;
            reset = <SPI8_RST>;
			label = "SPI_8";
			clocks = <&pcc SPI8_MODULE CLK_CLKSEL4_SPI8SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        spi9: spi@4006c000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x4006c000 0x6c>;
			interrupts = <111 0>;
            reset = <SPI9_RST>;
			label = "SPI_9";
			clocks = <&pcc SPI9_MODULE CLK_CLKSEL4_SPI9SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        spi10: spi@4006d000 {
			compatible = "nuvoton,numaker-spi";
			reg = <0x4006d000 0x6c>;
			interrupts = <119 0>;
            reset = <SPI10_RST>;
			label = "SPI_10";
			clocks = <&pcc SPI10_MODULE CLK_CLKSEL4_SPI10SEL_HIRC 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

        epwm0: epwm@40058000 {
			compatible = "nuvoton,numaker-pwm";
			reg = <0x40058000 0x37c>;
			interrupts = <24 0>, <25 0>, <26 0>, <27 0>;
            interrupt-names = "brake", "pair0", "pair1", "pair2";
            reset = <EPWM0_RST>;
            prescaler = <19>;
			label = "EPWM_0";
			clocks = <&pcc EPWM0_MODULE CLK_CLKSEL2_EPWM0SEL_PCLK0 0>;
			#pwm-cells = <3>;
			status = "disabled";
		};

        epwm1: epwm@40059000 {
			compatible = "nuvoton,numaker-pwm";
			reg = <0x40059000 0x37c>;
			interrupts = <28 0>, <29 0>, <30 0>, <31 0>;
            interrupt-names = "brake", "pair0", "pair1", "pair2";
            reset = <EPWM1_RST>;
            prescaler = <19>;
			label = "EPWM_1";
			clocks = <&pcc EPWM1_MODULE CLK_CLKSEL2_EPWM1SEL_PCLK1 0>;
			#pwm-cells = <3>;
			status = "disabled";
		};

        usbd: usbd@400c0000 {
			compatible = "nuvoton,numaker-usbd";
			reg = <0x400c0000 0x1000>;
			interrupts = <53 0>;
			reset = <USBD_RST>;
			clocks = <&pcc USBD_MODULE CLK_CLKSEL0_USBSEL_PLL_DIV2 CLK_CLKDIV0_USB(2)>;
			dma-buffer-size = <1536>;
            status = "disabled";
			label = "USBD";
			num-bidir-endpoints = <25>;
			disallow-iso-in-out-same-number;
		};

        hsusbd: hsusbd@40019000 {
			compatible = "nuvoton,numaker-hsusbd";
			reg = <0x40019000 0x1000>;
			interrupts = <65 0>;
			reset = <HSUSBD_RST>;
			clocks = <&pcc HSUSBD_MODULE 0 0>;
            dma-buffer-size = <4096>;
			status = "disabled";
			label = "HSUSBD";
			num-bidir-endpoints = <(12 + 2)>; /* Including control endpoints by +2 */
		};

		emac: ethernet@40012000 {
			compatible = "nuvoton,numaker-ethernet";
			reg = <0x40012000 0x105C>;
			interrupts = <66 0>;
            reset = <EMAC0_RST>;
			status = "disabled";
			label = "EMAC_0";
			phy-addr = <1>;
			clocks = <&pcc EMAC0_MODULE 0 0>;
		};
	};
};

&nvic {
	arm,num-irq-priority-bits = <4>;
};
