/*
 * Memory configuration: Flash size, RAM Size, Data Flash size
 * and additional nodes for 5164 memory variants
 */

/ {
	soc {
		flash0: flash@0 {
			reg = <0x0 DT_SIZE_K(512)>;
		};

		sram0: memory@20000000 {
			reg = <0x20000000 DT_SIZE_K(64)>;
		};
	};
};
