/* Device-tree nodes specific to 64-pin package variants */

#include <microchip/pic32c/pic32cm_jh/common/pic32cm_jh.dtsi>

/ {
	soc {
		portb: gpio@41000080 {
			compatible = "microchip,port-g1-gpio";
			reg = <0x41000080 0x80>;
			gpio-controller;
			#gpio-cells = <2>;
			#microchip,pin-cells = <2>;
		};
	};
};
