/* Device-tree nodes specific to 128-pin package variants */

#include <microchip/pic32c/pic32cx_sg/common/pic32cx_sg.dtsi>

/ {
	soc {
		portc: gpio@41008100 {
			status = "disabled";
			compatible = "microchip,port-g1-gpio";
			reg = <0x41008100 0x80>;
			gpio-controller;
			#gpio-cells = <2>;
			#microchip,pin-cells = <2>;
		};

		portd: gpio@41008180 {
			status = "disabled";
			compatible = "microchip,port-g1-gpio";
			reg = <0x41008180 0x80>;
			gpio-controller;
			#gpio-cells = <2>;
			#microchip,pin-cells = <2>;
		};
	};
};
