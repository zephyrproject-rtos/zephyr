	pll1_directo: pll1-directo@40000560 {
		compatible = "nxp,syscon-clock-mux";
		#clock-cells = <1>;
		/* SYSCON::PLL1CTRL[BYPASSPOSTDIV] */
		reg = <0x40000560 0x1>;
