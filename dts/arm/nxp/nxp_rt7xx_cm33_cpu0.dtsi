			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};
	};

	/* LPFlexcomm14/16 only support LPSPI function  */
	lpspi14: spi@484000 {
		compatible = "nxp,lpspi";
		reg = <0x484000 0x1000>;
