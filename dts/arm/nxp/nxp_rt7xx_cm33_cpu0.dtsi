			reg = <0x19d000 0x1000>;
			clocks = <&clkctl0 MCUX_FLEXCOMM12_CLK>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};
	};

	flexcomm13: flexcomm@19e000 {
		compatible = "nxp,lp-flexcomm";
		reg = <0x19e000 0x1000>;
		interrupts = <52 0>;
		status = "disabled";

		/* Empty ranges property implies parent and child address space is identical */
		ranges = <>;
