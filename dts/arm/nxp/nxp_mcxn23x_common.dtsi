	};

	fmu: flash-controller@43000 {
		compatible = "nxp,msf1";
		reg = <0x43000 0x1000>;
		interrupts = <138 0>;
		status = "disabled";

		#address-cells = <1>;
		#size-cells = <1>;

		flash: flash@0 {
			compatible = "soc-nv-flash";
			reg = <0 DT_SIZE_M(1)>;
			erase-block-size = <8192>;
			/* MCXN23x ROM Flash API supports writing of 128B pages. */
			write-block-size = <128>;
		};
	};

