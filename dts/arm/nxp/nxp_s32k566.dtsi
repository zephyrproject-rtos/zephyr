				reg-names = "pgpdo", "mscr";
				interrupt-parent = <&eirq1>;
				interrupts = <1 1>, <2 11>, <3 3>, <4 3>,
					     <5 5>, <6 2>, <7 12>, <9 9>,
					     <10 10>, <11 4>, <13 7>, <15 8>;
				gpio-controller;
				#gpio-cells = <2>;
				ngpios = <16>;
				status = "disabled";
			};

			gpiog: gpio@4020570e {
