				reg-names = "pgpdo", "mscr";
				interrupt-parent = <&eirq1>;
				interrupts = <0 9>, <1 0>, <2 10>, <3 1>,
					     <4 13>, <5 14>, <6 15>, <8 5>,
					     <9 2>, <10 4>, <13 6>, <14 7>,
					     <15 6>;
				gpio-controller;
				#gpio-cells = <2>;
				ngpios = <16>;
				status = "disabled";
			};

			gpioh: gpio@4020570c {
				compatible = "nxp,siul2-gpio";
