				interrupt-parent = <&eirq1>;
				interrupts = <0 8>, <4 15>, <14 11>, <15 12>;
				gpio-controller;
				#gpio-cells = <2>;
				ngpios = <16>;
				status = "disabled";
			};
		};

		/* LPE_SIUL2 */
