				interrupts = <2 13>, <3 10>, <4 9>, <5 9>, <6 2>,
					     <9 0>, <11 1>, <14 3>;
				gpio-controller;
				#gpio-cells = <2>;
				ngpios = <16>;
				status = "disabled";
			};
		};

		siul2_3: siul2@40610000 {
