				interrupts = <0 8>, <2 5>, <3 5>, <4 15>, <7 2>,
					     <8 4>, <10 3>, <11 14>, <14 3>, <15 6>;
				gpio-controller;
				#gpio-cells = <2>;
				ngpios = <16>;
				status = "disabled";
			};

			gpiom: gpio@4208d71a {
				compatible = "nxp,siul2-gpio";
				reg = <0x4208d71a 0x02>, <0x4208c540 0x40>;
				reg-names = "pgpdo", "mscr";
