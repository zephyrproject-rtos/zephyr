/dts-v1/;
#include <st/stm32f401.dtsi>
#include <i2c.h>

/ {
	model = "Seeed Studio Carbon 96boards";
	compatible = "seeed,carbon", "st,stm32f401";

	chosen {
		zephyr,console = &usart1;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};

&usart1 {
	current-speed = <115200>;
	status = "ok";
};

&usart2 {
	current-speed = <115200>;
	status = "ok";
};

&i2c1 {
	zephyr,speed = <I2C_SPEED_FAST>;
	status = "ok";
};
