flash_controller: flash-controller@39000 {
	compatible = "nordic,nrf91-flash-controller";
	reg = <0x39000 0x1000>;

	#address-cells = <1>;
	#size-cells = <1>;

	label="NRF_FLASH_DRV_NAME";

	flash0: flash@0 {
		compatible = "soc-nv-flash";
		label = "NRF_FLASH";
		erase-block-size = <4096>;
		write-block-size = <4>;
	};
};

adc: adc@e000 {
	compatible = "nordic,nrf-saadc";
	reg = <0xe000 0x1000>;
	interrupts = <14 5>;
	status = "disabled";
	label = "ADC_0";
	#io-channel-cells = <1>;
};

