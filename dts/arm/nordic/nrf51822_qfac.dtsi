#include <mem.h>
#include <nordic/nrf51822.dtsi>

&flash0 {
	reg = <0x00000000 DT_SIZE_K(256)>;
};

&sram0 {
	reg = <0x20000000 DT_SIZE_K(32)>;
};

/ {
	soc {
		compatible = "nordic,nrf51822-qfac", "nordic,nrf51822",
			     "nordic,nrf51", "simple-bus";
	};
};
