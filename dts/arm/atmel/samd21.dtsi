#include <atmel/samd.dtsi>

/ {
	clocks {
		fdpll: fdpll {
			compatible = "atmel,sam0-fdpll";
			#clock-cells = <0>;
			clock-output-names = "FDPLL";
			status = "disabled";
		};

		gclk8: gclk8 {
			compatible = "atmel,sam0-gclk";
			status = "disabled";
			#clock-cells = <0>;
			clock-output-names = "GCLK_8";
		};
	};

	aliases {
		tc-6 = &tc6;
	};

	soc {
		usb0: usb@41005000 {
			compatible = "atmel,sam0-usb";
			status = "disabled";
			reg = <0x41005000 0x1000>;
			interrupts = <7 0>;
			num-bidir-endpoints = <8>;
			label = "USB0";
			clocks = <&gclk0>;
		};

		dma: dmac@41004800 {
			compatible = "atmel,sam0-dmac";
			reg = <0x41004800 0x50>;
			interrupts = <6 0>;
			label = "DMA_0";
		};

		tc6: tc@42003800 {
			compatible = "atmel,sam0-tc32";
			status = "disabled";
			reg = <0x42003800 0x20>;
			interrupts = <21 0>;
			label = "TIMER_6";
			clocks = <&gclk0>;
			clock-frequency = <48000000>;
		};
	};
};

&sercom0 {
	interrupts = <9 0>;
};

&sercom1 {
	interrupts = <10 0>;
};

&sercom2 {
	interrupts = <11 0>;
};

&sercom3 {
	interrupts = <12 0>;
};

&sercom4 {
	interrupts = <13 0>;
};

&sercom5 {
	interrupts = <14 0>;
};

&tc4 {
	interrupts = <19 0>;
};

&adc {
	interrupts = <23 0>;
};
