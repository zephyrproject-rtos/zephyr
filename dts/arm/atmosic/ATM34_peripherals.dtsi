timer0: timer@0 {
	compatible = "arm,cmsdk-timer";
	reg = <0x00000000 0x1000>;
	interrupts = <3 8>;
};

timer1: timer@1000 {
	compatible = "arm,cmsdk-timer";
	reg = <0x00001000 0x1000>;
	interrupts = <4 8>;
};

dtimer0: dtimer@2000 {
	compatible = "arm,cmsdk-dtimer";
	reg = <0x00002000 0x1000>;
	interrupts = <5 8>;
};

wdog0: wdog@81000 {
	compatible = "arm,cmsdk-watchdog";
	clocks = <&sysclk>;
	reg = <0x00081000 0x1000>;
};

gpio0: gpio@100000 {
	compatible = "arm,cmsdk-gpio";
	reg = <0x00100000 0x1000>;
	interrupts = <93 8>;
	gpio-controller;
	#gpio-cells = <2>;
};

gpio1: gpio@101000 {
	compatible = "arm,cmsdk-gpio";
	reg = <0x00101000 0x1000>;
	interrupts = <94 8>;
	gpio-controller;
	#gpio-cells = <2>;
};

gpio2: gpio@102000 {
	compatible = "arm,cmsdk-gpio";
	reg = <0x00102000 0x1000>;
	interrupts = <58 8>;
	gpio-controller;
	#gpio-cells = <2>;
};

gpio3: gpio@104000 {
	compatible = "arm,cmsdk-gpio";
	reg = <0x00104000 0x1000>;
	interrupts = <59 8>;
	gpio-controller;
	#gpio-cells = <2>;
};

ble: ble@120000 {
	compatible = "atmosic,ble";
	reg = <0x00120000 0x10000>;
	interrupts = <72 4>, <75 8>, <80 8>, <95 2>;
	status = "okay";
};

ieee802154: mac_154 {
	compatible = "atmosic,atm34-ieee802154";
	status = "disabled";
};

wrpr0: wrpr0@143000 {
	reg = <0x00143000 0x1000>;
};

uart0: uart@144000 {
	compatible = "atmosic,atm-uart";
	reg = <0x00144000 0x1000>;
	interrupts = <32 8>, <33 8>, <41 8>;
	interrupt-names = "rx", "tx", "ovf";
	clocks = <&clk_16M>;
	current-speed = <115200>;
	instance = <0>;
	status = "disabled";
};

uart1: uart@145000 {
	compatible = "atmosic,atm-uart";
	reg = <0x00145000 0x1000>;
	interrupts = <34 8>, <35 8>, <42 8>;
	interrupt-names = "rx", "tx", "ovf";
	clocks = <&clk_16M>;
	current-speed = <115200>;
	instance = <1>;
};

uart2: uart@168000 {
	compatible = "atmosic,atm-uart";
	reg = <0x00168000 0x1000>;
	interrupts = <62 8>, <63 8>, <57 8>;
	interrupt-names = "rx", "tx", "ovf";
	clocks = <&clk_16M>;
	current-speed = <115200>;
	instance = <2>;
	status = "disabled";
};

