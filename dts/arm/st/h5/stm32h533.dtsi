#include <st/h5/stm32h5.dtsi>

/ {
	soc {
		compatible = "st,stm32h533", "st,stm32h5", "simple-bus";

		gpiof: gpio@42021400 {
			compatible = "st,stm32-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x42021400 0x400>;
			clocks = <&rcc STM32_CLOCK_BUS_AHB2 0x00000080>;
		};
	};
};
