#include <st/wl/stm32wle5.dtsi>

/ {
	sram0: memory@20000000 {
		reg = <0x20000000 DT_SIZE_K(48)>;
	};

	soc {
		flash-controller@58004000 {
			flash0: flash@8000000 {
				reg = <0x08000000 DT_SIZE_K(128)>;
				ranges = <0x0 0x8000000 DT_SIZE_K(128)>;
				#address-cells = <1>;
				#size-cells = <1>;
			};
		};
	};
};
