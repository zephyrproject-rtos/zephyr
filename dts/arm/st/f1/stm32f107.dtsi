#include <st/f1/stm32f105.dtsi>

/ {
	soc {
		dma2: dma@40020400 {
			compatible = "st,stm32-dma-v2";
			reg = <0x40020400 0x400>;
			clocks = <&rcc STM32_CLOCK_BUS_AHB1 0x2>;
			interrupts = <56 0 57 0 58 0 59 0 60 0>;
			status = "disabled";
			label = "DMA_2";
		};
	};
};
