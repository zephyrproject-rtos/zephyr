#include <st/f0/stm32f0.dtsi>

/ {
	soc {
		compatible = "st,stm32f030", "st,stm32f0", "simple-bus";
	};

	die_temp: dietemp {
		compatible = "st,stm32c0-temp-cal";
		ts-cal1-addr = <0x1FFFF7B8>;
		ts-cal1-temp = <30>;
		ntc;
		ts-cal-vrefanalog = <3300>;
		io-channels = <&adc1 16>;
		status = "disabled";
	};
};
