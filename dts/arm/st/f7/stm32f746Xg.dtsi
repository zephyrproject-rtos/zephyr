#include <mem.h>
#include <st/f7/stm32f746.dtsi>

/ {
	soc {
		flash-controller@40023c00 {
			flash0: flash@8000000 {
				reg = <0x08000000 DT_SIZE_K(1024)>;
			};
		};
	};

	otghs_ulpi_phy: otghs_ulpis_phy {
		compatible = "usb-ulpi-phy";
		#phy-cells = <0>;
		clocks = <&rcc STM32_CLOCK(AHB1, 30)>;
	};

};
