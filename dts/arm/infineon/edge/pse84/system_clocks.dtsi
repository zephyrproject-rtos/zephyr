	clk_iho: clk_iho {
		#clock-cells = <0>;
		compatible = "infineon,fixed-clock";
		clock-frequency = <12000000>;
		system-clock = <IFX_IHO>;
		status = "okay";
	};

	/* pilo */
	clk_pilo: clk_pilo {
		#clock-cells = <0>;
		compatible = "infineon,fixed-clock";
		clock-frequency = <32768>;
		system-clock = <IFX_PILO>;
		status = "okay";
	};

	dpll_hp: dpll_hp {
