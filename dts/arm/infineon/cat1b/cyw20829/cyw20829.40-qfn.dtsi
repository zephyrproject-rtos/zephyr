				pinmux = <DT_CAT1_PINMUX(5, 1, HSIOM_SEL_ACT_7)>;
			};

			/* scb_spi_m_clk */
			/omit-if-no-ref/ p0_4_scb0_spi_m_clk: p0_4_scb0_spi_m_clk {
				pinmux = <DT_CAT1_PINMUX(0, 4, HSIOM_SEL_DS_6)>;
