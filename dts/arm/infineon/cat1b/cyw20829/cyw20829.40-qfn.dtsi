				pinmux = <DT_CAT1_PINMUX(3, 1, HSIOM_SEL_ACT_8)>;
			};

			/* scb_spi_s_miso */
			/omit-if-no-ref/ p1_3_scb1_spi_s_miso: p1_3_scb1_spi_s_miso {
				pinmux = <DT_CAT1_PINMUX(1, 3, HSIOM_SEL_ACT_8)>;
