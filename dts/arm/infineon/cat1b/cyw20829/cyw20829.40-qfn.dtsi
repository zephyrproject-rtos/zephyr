			/omit-if-no-ref/ p3_3_scb1_spi_m_miso: p3_3_scb1_spi_m_miso {
				pinmux = <DT_CAT1_PINMUX(3, 3, HSIOM_SEL_ACT_8)>;
			};
			/omit-if-no-ref/ p4_1_scb0_spi_m_miso: p4_1_scb0_spi_m_miso {
				pinmux = <DT_CAT1_PINMUX(4, 1, HSIOM_SEL_DS_6)>;
