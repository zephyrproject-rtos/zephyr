#include <arm/armv8-m.dtsi>
/ {
	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-m33";
			reg = <0>;
			clock-frequency = <180000000>;
		};
	};
};

&nvic {
	arm,num-irq-priority-bits = <3>;
};
