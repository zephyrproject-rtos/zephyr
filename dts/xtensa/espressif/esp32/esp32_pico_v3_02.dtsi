#include "esp32_common.dtsi"

/* Reserved GPIO pins */
&gpio0 {
	gpio-reserved-ranges = <6 6>, // flash
			<24 2>, <28 4>; // NC
};

/* 8MB flash */
&flash0 {
	reg = <0x0 DT_SIZE_M(8)>;
};

/* 2MB psram */
&psram0 {
	reg = <0x3f800000 DT_SIZE_M(2)>;
	status = "okay";
};
