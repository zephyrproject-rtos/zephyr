#include "esp32_common.dtsi"

/* Reserved GPIO pins */
&gpio0 {
	gpio-reserved-ranges = <2 1>, <25 1>, // NC/test
				<6 6>, // flash
				<20 1>, <24 1>, <28 4>; // NC
};

&gpio1 {
	gpio-reserved-ranges = <6 2>; // GPIO37-38 NC
};

/* 4MB flash */
&flash0 {
	reg = <0x0 DT_SIZE_M(4)>;
};
