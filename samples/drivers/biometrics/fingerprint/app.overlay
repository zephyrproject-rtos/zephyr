 * Sample devicetree overlay for a fingerprint sensor
 */

/ {
	aliases {
		fingerprint = &fingerprint0;
	};
};

&uart1 {
	status = "okay";
	current-speed = <57600>;

	fingerprint0: fingerprint0 {
		compatible = "zhiantec,zfm-x0";
		status = "okay";
		comm-addr = <0xffffffff>;
	};
};
