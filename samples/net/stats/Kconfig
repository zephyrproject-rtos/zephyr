mainmenu "Network statistics sample application"

config SAMPLE_PERIOD
	int "How often to print statistics (in seconds)"
	default 30
	range 1 3600
	depends on NET_STATISTICS
	help
	  Print statistics after every n. seconds


source "Kconfig.zephyr"
