/ {
	aliases {
		wkup-src = &wkup_button;
	};

	gpio_keys {
		wkup_button: wkup {
			label = "WKUP";
			gpios = <&gpioa 0 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>;
		};
	};
};

&pwr {
	status = "okay";
};
