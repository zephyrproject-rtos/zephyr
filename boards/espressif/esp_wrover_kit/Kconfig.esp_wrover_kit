config BOARD_ESP_WROVER_KIT
	select SOC_ESP32_WROVER_E_N4R8
	select SOC_ESP32_PROCPU if BOARD_ESP_WROVER_KIT_ESP32_PROCPU
	select SOC_ESP32_APPCPU if BOARD_ESP_WROVER_KIT_ESP32_APPCPU
