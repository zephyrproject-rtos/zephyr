config BOARD_ESP32S3_DEVKITC
	select SOC_ESP32S3_WROOM_N8 if BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU	|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU	|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N8 || \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N8
	select SOC_ESP32S3_WROOM_N4R2 if BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N4R2 || \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N4R2
	select SOC_ESP32S3_WROOM_N8R2 if BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N8R2 || \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N8R2
	select SOC_ESP32S3_WROOM_N8R8 if BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N8R8 || \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N8R8
	select SOC_ESP32S3_WROOM_N16R2 if BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N16R2 || \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N16R2
	select SOC_ESP32S3_WROOM_N16R8 if BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N16R8 || \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N16R8
	select SOC_ESP32S3_PROCPU if BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU		|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N4R2	|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N8		|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N8R2	|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N8R8	|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N16R2	|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_PROCPU_N16R8
	select SOC_ESP32S3_APPCPU if BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU		|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N4R2	|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N8		|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N8R2	|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N8R8	|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N16R2	|| \
					BOARD_ESP32S3_DEVKITC_ESP32S3_APPCPU_N16R8
