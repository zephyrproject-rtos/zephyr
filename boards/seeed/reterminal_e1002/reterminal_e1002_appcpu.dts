/dts-v1/;

#include <espressif/esp32s3/esp32s3_r8.dtsi>
#include <espressif/partitions_0x0_amp_32M.dtsi>

/ {
	model = "Seeed Studio reTerminal E1002 APPCPU";
	compatible = "seeed,reterminal-e1002";

	chosen {
		zephyr,sram = &sram1;
		zephyr,ipc_shm = &shm0;
		zephyr,ipc = &ipm0;
		zephyr,flash = &flash0;
		zephyr,code-partition = &slot0_appcpu_partition;
	};
};

&flash0 {
	reg = <0x0 DT_SIZE_M(32)>;
};

&trng0 {
	status = "okay";
};

&ipm0 {
	status = "okay";
};
