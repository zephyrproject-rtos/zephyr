/dts-v1/;

#include "nsim-smp.dtsi"
#include "nsim-flat-mem.dtsi"

/ {
	model = "snps,nsim_hs";
	compatible = "snps,nsim_hs";

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "snps,arcv3-hs";
			reg = <0>;
		};

		cpu@1 {
			device_type = "cpu";
			compatible = "snps,arcv3-hs";
			reg = <1>;
		};
	};
};
