config BOARD_EM_STARTERKIT
	select SOC_EMSK
	select SOC_EMSK_EM7D if BOARD_EM_STARTERKIT_EMSK_EM7D
	select SOC_EMSK_EM9D if BOARD_EM_STARTERKIT_EMSK_EM9D
	select SOC_EMSK_EM11D if BOARD_EM_STARTERKIT_EMSK_EM11D
