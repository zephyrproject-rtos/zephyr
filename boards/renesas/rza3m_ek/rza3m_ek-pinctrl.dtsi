#include <zephyr/dt-bindings/gpio/gpio.h>
#include <zephyr/dt-bindings/pinctrl/renesas/pinctrl-rza3m.h>

&pinctrl {
	/omit-if-no-ref/ scif0_pins: scif0 {
		scif0-pinmux {
			pinmux = <RZA_PINMUX(PORT_06, 0, 2)>, /* RXD */
				 <RZA_PINMUX(PORT_06, 1, 2)>; /* TXD */
			drive-strength = <1>;
			slew-rate = "fast";
		};
	};
};
