if BOARD_NRF52840_PCA10056

config BOARD
	default "nrf52840_pca10056"

if USB

config USB_NRF52840
	default y

config USB_DEVICE_STACK
	default y

endif # USB

if IEEE802154

config IEEE802154_NRF5
	default y

endif # IEEE802154

config SOC_NRF52X_HAS_DCDC_SUPPORT
    default y

endif # BOARD_NRF52840_PCA10056
