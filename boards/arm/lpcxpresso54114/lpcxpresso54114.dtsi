/ {
	aliases{
		usart-0 = &usart0;
		mailbox-0 = &mailbox0;
		led0 = &red_led;
		led1 = &green_led;
		led2 = &blue_led;
	};

	leds {
		compatible = "gpio-leds";
		red_led: led_0 {
			gpios = <&gpio0 29 0>;
			label = "User LD1";
			status = "disabled";
		};
		green_led: led_1 {
			gpios = <&gpio1 10 0>;
			label = "User LD2";
			status = "disabled";
		};
		blue_led: led_2 {
			gpios = <&gpio1 9 0>;
			label = "User LD3";
			status = "disabled";
		};
	};
};
