config BOARD_ENABLE_DCDC
	bool "DCDC mode"
	select SOC_DCDC_NRF52X
	default y
	depends on BOARD_ADAFRUIT_ITSYBITSY_NRF52840

config BOARD_SERIAL_BACKEND_CDC_ACM
	bool "USB CDC"
	default y
