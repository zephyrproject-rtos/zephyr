/dts-v1/;

#include <nxp/nxp_k6x.dtsi>

/ {

	model = "Teensy Explorer Board";
	compatible = "nxp,mk66f18","nxp,mk66f","nxp,mk64f12", "nxp,k64f", "nxp,k6x";

	aliases {
		led0 = &teensy_led;
		led1 = &red_led;
		led2 = &green_led;
		led3 = &blue_led;
		sw0 = &user_button_3;
		sw1 = &user_button_2;
		can-primary = &can0;
	};

	chosen {
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,code-partition = &code_partition;
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
		zephyr,uart-pipe = &uart0;
	};

	leds {
		compatible = "gpio-leds";
		teensy_led: led_0 {
			gpios = <&gpioc 5 GPIO_ACTIVE_LOW>;
			label = "User LD1";
		};
		red_led: led_1 {
			gpios = <&gpiod 15 GPIO_ACTIVE_HIGH>;
			label = "User LD2";
		};
		green_led: led_2 {
			gpios = <&gpiod 12 GPIO_ACTIVE_HIGH>;
			label = "User LD3";
		};
		blue_led: led_3 {
			gpios = <&gpiod 11 GPIO_ACTIVE_HIGH>;
			label = "User LD3";
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		user_button_2: button_0 {
			label = "User SW2";
			gpios = <&gpioe 10 GPIO_ACTIVE_LOW>;
		};
		user_button_3: button_1 {
			label = "User SW3";
			gpios = <&gpioe 11 GPIO_ACTIVE_LOW>;
		};
	};
};

&cpu0 {
	clock-frequency = <120000000>;
};

&adc0 {
	status = "okay";
};

&adc1 {
	status = "okay";
};

&temp1 {
	status = "okay";
};

&i2c0 {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&pwm3 {
	status = "okay";
};

&uart0 {
	status = "okay";
	current-speed = <115200>;
};

&sas_uart {
	status = "okay";
	current-speed = <19200>;
};

&usbd {
	compatible = "nxp,kinetis-usbd";
	status = "okay";
	num-bidir-endpoints = <8>;
};

&eth {
	status = "okay";
	ptp {
		status = "okay";
	};
};

&can0 {
	status = "okay";
	bus-speed = <125000>;
};
