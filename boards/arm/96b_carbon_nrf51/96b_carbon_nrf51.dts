/dts-v1/;
#include <nordic/nrf51822_qfac.dtsi>

/ {
	model = "Seeed Studio Carbon nRF51 96board";
	compatible = "seeed,carbon_nrf51", "nordic,nrf51822-qfac",
		     "nordic,nrf51822";

	chosen {
		zephyr,console = &uart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};

&gpiote {
	status ="ok";
};

&gpio0 {
	status ="ok";
};

&uart0 {
	current-speed = <115200>;
	status = "ok";
	tx-pin = <29>;
	rx-pin = <11>;
};

&spi1 {
	status = "ok";
	sck-pin = <7>;
	mosi-pin = <0>;
	miso-pin = <30>;
	csn-pin = <25>;
};
