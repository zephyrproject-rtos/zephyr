/dts-v1/;

#include <gigadevice/gd32f403/gd32f403zet6.dtsi>

/ {
	model = "GigaDevice GD32F403Z Evaluation Kit";
	compatible = "gd,gd32f403z";

	chosen {
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,console = &usart0;
		zephyr,shell-uart = &usart0;
	};
};

&usart0 {
	status = "okay";

	current-speed = <115200>;
};
