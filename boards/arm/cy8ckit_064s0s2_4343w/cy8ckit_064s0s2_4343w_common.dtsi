/ {
	aliases {
		led0 = &user_rgb_green;
		sw0 = &user_bt;
	};

	leds {
		compatible = "gpio-leds";
		user_led: led_Orange {
			label = "LED_Orange";
			gpios = <&gpio_prt1 5 GPIO_ACTIVE_HIGH>;
		};
		user_led2: led_Red {
			label = "LED_Red";
			gpios = <&gpio_prt11 1 GPIO_ACTIVE_HIGH>;
		};
		user_rgb_red: rgb_Red {
			label = "RGB_Red";
			gpios = <&gpio_prt1 1 GPIO_ACTIVE_HIGH>;
		};
		user_rgb_green: rgb_Green {
			label = "RGB_Green";
			gpios = <&gpio_prt0 5 GPIO_ACTIVE_HIGH>;
		};
		user_rgb_blue: rgb_Blue {
			label = "RGB_Blue";
			gpios = <&gpio_prt7 3 GPIO_ACTIVE_HIGH>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";

		user_bt: button_0 {
			label = "SW_0";
			gpios = <&gpio_prt0 4 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
		};
		user_bt2: button_2 {
			label = "SW_1";
			gpios = <&gpio_prt1 4 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
		};
	};
};

&gpio_prt0 {
	status = "okay";
};

&gpio_prt1 {
	status = "okay";
};

&gpio_prt3 {
	status = "okay";
};

&gpio_prt5 {
	status = "okay";
