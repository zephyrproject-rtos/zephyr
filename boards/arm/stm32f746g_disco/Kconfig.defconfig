if BOARD_STM32F746G_DISCO

config BOARD
	default "stm32f746g_disco"

config UART_1
	default y
	depends on UART_CONSOLE

if NETWORKING

config NET_L2_ETHERNET
	default y

config ETH_STM32_HAL
	default y

endif # NETWORKING

config UART_6
	default y
	depends on SERIAL


config PWM_STM32_1
	default y
	depends on PWM

endif # BOARD_STM32F746G_DISCO
