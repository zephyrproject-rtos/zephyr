/* Add I2C1 support on Particle Mesh via nRF52840 TWI1
 *
 * NOTE: This file is replicated in particle_{argon,xenon}.
 * Changes should be made in all instances. */
&i2c1 { /* feather I2C1 */
	status = "okay";
	sda-gpios = <&gpio1 1 0>;
	scl-gpios = <&gpio1 2 0>;
};

