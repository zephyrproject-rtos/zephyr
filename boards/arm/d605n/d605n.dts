        wchargerleden = &wchargerleden;
        blinken = &blinken;

        flashpwren = &flashpwren;
        wakeupintn = &wakeupintn;
        usbd0 = &usbd;

        resetall   = &resetall;
        slot3reset = &slot3reset;
        slot4reset = &slot4reset;
        slot5reset = &slot5reset;

