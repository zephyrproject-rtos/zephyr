    silego_expander_pins {
        compatible = "gpio-keys";

        silegobit0: silego_bit_0 { gpios = <&gpio0 0 (GPIO_ACTIVE_LOW)>; label = ""; };
        silegobit1: silego_bit_1 { gpios = <&gpio0 1 (GPIO_ACTIVE_LOW)>; label = ""; };
