    };

    silego_expander_masks {
        compatible = "gpio-keys";
        
        resetall: reset_all { gpios = <&gpio0 0 (GPIO_ACTIVE_LOW)>; label = ""; };
        slot3reset: slot_3_reset { gpios = <&gpio0 1 (GPIO_ACTIVE_LOW)>; label = ""; };
