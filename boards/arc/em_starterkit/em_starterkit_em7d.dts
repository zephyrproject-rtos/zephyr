/dts-v1/;

#include <emsk.dtsi>

/ {
	model = "em_starterkit-em7d";
	compatible = "snps,em_starterkit-em7d", "snps,em_starterkit";

	aliases {
		uart-0 = &uart0;
		uart-1 = &uart1;
		uart-2 = &uart2;
	};

	chosen {
		zephyr,sram = &ddr0;
		zephyr,console = &uart1;
	};
};

&uart1 {
	status = "ok";
	current-speed = <115200>;
};
