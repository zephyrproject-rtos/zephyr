config BOARD_M5STACK_CORES3
	select SOC_ESP32S3
	select SOC_ESP32S3_PROCPU if BOARD_M5STACK_CORES3_ESP32S3_PROCPU || \
				     BOARD_M5STACK_CORES3_ESP32S3_PROCPU_SE
	select SOC_ESP32S3_APPCPU if BOARD_M5STACK_CORES3_ESP32S3_APPCPU
