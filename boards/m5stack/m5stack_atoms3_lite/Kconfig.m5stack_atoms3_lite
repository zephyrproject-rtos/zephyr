config BOARD_M5STACK_ATOMS3_LITE
	select SOC_ESP32S3_FN8
	select SOC_ESP32S3_PROCPU if BOARD_M5STACK_ATOMS3_LITE_ESP32S3_PROCPU
	select SOC_ESP32S3_APPCPU if BOARD_M5STACK_ATOMS3_LITE_ESP32S3_APPCPU
