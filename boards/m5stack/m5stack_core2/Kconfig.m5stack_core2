config BOARD_M5STACK_CORE2
	select SOC_ESP32_D0WD_V3
	select SOC_ESP32_PROCPU if BOARD_M5STACK_CORE2_ESP32_PROCPU
	select SOC_ESP32_APPCPU if BOARD_M5STACK_CORE2_ESP32_APPCPU
