config BOARD_M5STICKC_PLUS
	select SOC_ESP32_PICO_D4
	select SOC_ESP32_PROCPU if BOARD_M5STICKC_PLUS_ESP32_PROCPU
	select SOC_ESP32_APPCPU if BOARD_M5STICKC_PLUS_ESP32_APPCPU
