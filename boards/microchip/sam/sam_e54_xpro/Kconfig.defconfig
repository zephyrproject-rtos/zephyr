if ETH_MCHP_GMAC_U2005

# Read MAC address from AT24MAC402 EEPROM

config ETH_MCHP_MAC_I2C_INT_ADDRESS
	default 0x9A

config ETH_MCHP_MAC_I2C_INT_ADDRESS_SIZE
	default 1

config ETH_MCHP_MAC_I2C_EEPROM
	default y
	select I2C

endif # ETH_MCHP_GMAC_U2005

if NETWORKING

config NET_L2_ETHERNET
	default y

endif # NETWORKING
