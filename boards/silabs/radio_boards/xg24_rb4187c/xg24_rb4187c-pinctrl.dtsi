#include <dt-bindings/pinctrl/silabs/xg24-pinctrl.h>

&pinctrl {
	usart0_default: usart0_default {
		group0 {
			pins = <USART0_TX_PA8>;
			drive-push-pull;
			output-high;
		};
		group1 {
			pins = <USART0_RX_PA9>;
			input-enable;
			silabs,input-filter;
		};
	};
};
