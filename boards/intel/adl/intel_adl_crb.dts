#include "intel_adl.dts"

/ {
	compatible = "intel,alder-lake-crb";
	model = "Intel Alder Lake CRB";

	chosen {
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
	};

	aliases {
		rtc = &rtc;
	};
};

&uart0 {
	status = "okay";
};
