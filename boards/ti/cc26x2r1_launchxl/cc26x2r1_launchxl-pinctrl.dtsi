#include <zephyr/dt-bindings/pinctrl/cc13xx_cc26xx-pinctrl.h>

&pinctrl {
	/* UART0 */
	uart0_tx_default: uart0_tx_default {
		pinmux = <3 IOC_PORT_MCU_UART0_TX>;
		bias-disable;
	};

	uart0_rx_default: uart0_rx_default {
		pinmux = <2 IOC_PORT_MCU_UART0_RX>;
		bias-disable;
		input-enable;
	};
};
