/dts-v1/;

#include <rockchip/rk3588.dtsi>
#include <common/mem.h>

/ {
	model = "Xunlong Orange Pi 5 Ultra";
	compatible = "xunlong,orangepi-5-ultra", "rockchip,rk3588";

	chosen {
		zephyr,sram = &sram0;
		zephyr,console = &uart2;
		zephyr,shell-uart = &uart2;
	};

	sram0: memory@0 {
		device_type = "memory";
		compatible = "mmio-sram";
		reg = <0x0 DT_SIZE_G(1)>;
	};
};

&uart2 {
	status = "okay";
};

&gic {
	status = "okay";
};
