# Set supported clock accuracy
CONFIG_SYS_CLOCK_TICKS_PER_SEC=4500

# Enable GPIO
CONFIG_GPIO=y

# Console
CONFIG_CONSOLE=y
CONFIG_UART_CONSOLE=y

# Enable UART
CONFIG_SERIAL=y
CONFIG_UART_INTERRUPT_DRIVEN=y

CONFIG_USE_DT_CODE_PARTITION=y
