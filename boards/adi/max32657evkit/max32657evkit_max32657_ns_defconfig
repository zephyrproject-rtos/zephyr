# Enable GPIO
CONFIG_GPIO=y

# Console
CONFIG_CONSOLE=y
CONFIG_UART_CONSOLE=y

# Enable UART
CONFIG_SERIAL=y
CONFIG_UART_INTERRUPT_DRIVEN=y

# It is non-secure fw, enable flags
CONFIG_TRUSTED_EXECUTION_NONSECURE=y

# Set TFM and Zephyr sign key
CONFIG_TFM_MCUBOOT_SIGNATURE_TYPE="RSA-3072"
