config BOARD_WEACT_ESP32S3_B
	select SOC_ESP32S3_WROOM_N8
	select SOC_ESP32S3_PROCPU if BOARD_WEACT_ESP32S3_B_ESP32S3_PROCPU
	select SOC_ESP32S3_APPCPU if BOARD_WEACT_ESP32S3_B_ESP32S3_APPCPU
