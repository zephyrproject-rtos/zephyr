/dts-v1/;

#include <broadcom/viper-a72.dtsi>

/ {
	model = "Broadcom BCM958402M2_A72";
	compatible = "brcm,viper";
	#address-cells = <1>;
	#size-cells = <1>;

	chosen {
		zephyr,console = &uart1;
		zephyr,shell-uart = &uart1;
		zephyr,sram = &sram0;
	};
};

&uart1 {
	status = "okay";
	current-speed = <57600>;
};
