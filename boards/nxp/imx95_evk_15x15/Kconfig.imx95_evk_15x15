config BOARD_IMX95_EVK_15X15
	select SOC_MIMX9596_A55 if BOARD_IMX95_EVK_15X15_MIMX9596_A55
	select SOC_MIMX9596_M7 if BOARD_IMX95_EVK_15X15_MIMX9596_M7
	# Use a part number that exists in MCUX device headers for both A55 and M7.
	select SOC_PART_NUMBER_MIMX9596AVZXN
