#include <nxp/kinetis/MKE17Z256VLL7-pinctrl.h>

&pinctrl {

	lpuart0_default: lpuart0_default {
		group0 {
			pinmux = <LPUART0_TX_PTB1>,
				<LPUART0_RX_PTB0>;
			drive-strength = "low";
			slew-rate = "slow";
		};
	};

	lpi2c0_default: lpi2c0_default {
		group0 {
			pinmux = <LPI2C0_SDA_PTA16>,
				<LPI2C0_SCL_PTB8>;
			bias-pull-up;
			drive-strength = "low";
			slew-rate = "slow";
		};
	};

	ftm2_default: ftm2_default {
		group0 {
			pinmux = <FTM2_CH0_PTD10>,
				<FTM2_CH1_PTD11>,
				<FTM2_CH2_PTD12>;
			drive-strength = "low";
			slew-rate = "slow";
		};
	};
};
