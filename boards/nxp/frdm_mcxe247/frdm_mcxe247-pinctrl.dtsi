#include <nxp/mcx/MCXE247VLQ-pinctrl.h>

&pinctrl {
	pinmux_lpuart1: pinmux_lpuart1 {
		group0 {
			pinmux = <LPUART1_RX_PTC8>,
				 <LPUART1_TX_PTC9>;
			drive-strength = "low";
			slew-rate = "fast";
			input-enable;
		};
	};

	pinmux_lpuart2: pinmux_lpuart2 {
		group0 {
			pinmux = <LPUART2_RX_PTD17>,
				 <LPUART2_TX_PTE12>;
			drive-strength = "low";
			slew-rate = "slow";
		};
	};

	pinmux_adc0: pinmux_adc0 {
		group0 {
			pinmux = <ADC0_SE1_PTA1>,
				 <ADC0_SE0_PTA0>;
			drive-strength = "low";
			slew-rate = "slow";
		};
	};

	pinmux_cmp0: pinmux_cmp0 {
		group0 {
			pinmux = <CMP0_IN0_PTA0>;
			drive-strength = "high";
			slew-rate = "fast";
		};
	};
};
