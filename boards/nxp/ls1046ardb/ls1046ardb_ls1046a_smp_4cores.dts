/dts-v1/;

#include <nxp/nxp_ls1046a.dtsi>

/ {
	model = "NXP ls1046a RDB";
	compatible = "nxp,ls1046ardb";

	chosen {
		zephyr,console = &uart1;
		zephyr,shell-uart = &uart1;
		zephyr,sram = &sram0;
	};
};

&uart1 {
	status = "okay";
	current-speed = <115200>;
};
