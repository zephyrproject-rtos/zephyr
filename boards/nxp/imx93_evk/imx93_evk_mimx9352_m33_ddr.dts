/dts-v1/;

#include "imx93_evk_mimx9352_m33.dts"

/ {
	model = "NXP i.MX93 EVK board DDR variant";

	chosen {
		zephyr,sram = &ddr;
		/delete-property/ zephyr,flash;
	};

	ddr: memory@84000000 {
		reg = <0x84000000 DT_SIZE_M(4)>;
		device_type = "memory";
	};
};
