# Enable GPIO
CONFIG_GPIO=y

# Enable UART driver
CONFIG_SERIAL=y
CONFIG_UART_INTERRUPT_DRIVEN=y

# Console (remote proc console by default)
CONFIG_CONSOLE=y

# UART console (overrides remote proc console)
CONFIG_UART_CONSOLE=y
