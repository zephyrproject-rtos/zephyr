if BOARD_STM32F746G_DISCO

configdefault NET_L2_ETHERNET
	default y

config INPUT
	default y if LVGL

if DISPLAY

# MEMC needs to be enabled in order to store
# display buffer to external SDRAM connected to FMC
config MEMC
	default y

endif # DISPLAY

endif # BOARD_STM32F746G_DISCO
