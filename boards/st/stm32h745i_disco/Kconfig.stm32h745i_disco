config BOARD_STM32H745I_DISCO
	select SOC_STM32H745XX_M7 if BOARD_STM32H745I_DISCO_STM32H745XX_M7
	select SOC_STM32H745XX_M4 if BOARD_STM32H745I_DISCO_STM32H745XX_M4
