# Enable UART driver
CONFIG_SERIAL=y

# Enable GPIO
CONFIG_GPIO=y

# Console
CONFIG_CONSOLE=y
CONFIG_UART_CONSOLE=y

# Enable MPU
CONFIG_ARM_MPU=y

# Enable Hardware stack protection
CONFIG_HW_STACK_PROTECTION=y

# No Internal Flash
CONFIG_XIP=n
