if BOARD_STM32H573I_DK

configdefault NET_L2_ETHERNET
	default y

configdefault SDMMC_STM32_CLOCK_CHECK
	default n

if DISPLAY

choice ST7789V_PIXEL_FORMAT
	default ST7789V_RGB565X
endchoice

# Required to enable LCD backlight
config REGULATOR
	default y

config I2C_STM32_V2_TIMING
	default y if INPUT

endif # DISPLAY

endif # BOARD_STM32H573I_DK
