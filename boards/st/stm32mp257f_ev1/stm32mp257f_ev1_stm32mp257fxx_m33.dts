/dts-v1/;
#include <st/mp2/stm32mp257_m33.dtsi>

/ {
	model = "STMicroelectronics STM32MP257F-EV1 board";
	compatible = "st,stm32mp257f-ev1", "st,stm32mp25";

	chosen {
		zephyr,flash = &ddr_code;
		zephyr,sram = &ddr_sys;
	};
};

&rcc {
	clock-frequency = <DT_FREQ_M(400)>;
};
