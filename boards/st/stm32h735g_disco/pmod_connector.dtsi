/ {
	pmod0: pmod-connector {
		compatible = "digilent,pmod";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map = <0 0 &gpiof 9 0>,	/* IO1 */
			   <1 0 &gpiof 7 0>,	/* IO2 */
			   <2 0 &gpiof 6 0>,	/* IO3 */
			   <3 0 &gpiof 8 0>,	/* IO4 */
			   <4 0 &gpioh 12 0>,	/* IO5 */
			   <5 0 &gpioh 1 0>;	/* IO6 */
						/* IO7 - not connected */
						/* IO8 - not connected */
	};
};
