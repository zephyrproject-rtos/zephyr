config BOARD_ESP32S3_ETH_8DI_8RO
	select SOC_ESP32S3_WROOM_N16R8
	select SOC_ESP32S3_PROCPU if BOARD_ESP32S3_ETH_8DI_8RO_ESP32S3_PROCPU || BOARD_ESP32S3_ETH_8DI_8RO_ESP32S3_PROCPU_C
	select SOC_ESP32S3_APPCPU if BOARD_ESP32S3_ETH_8DI_8RO_ESP32S3_APPCPU || BOARD_ESP32S3_ETH_8DI_8RO_ESP32S3_APPCPU_C
