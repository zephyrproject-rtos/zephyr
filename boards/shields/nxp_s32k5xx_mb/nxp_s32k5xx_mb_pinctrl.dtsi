#include <nxp/s32/S32K56X-437BGA-pinctrl.h>

&pinctrl {
	lpuart3_default: lpuart3_default {
		group1 {
			pinmux = <PI9_LPUART3_TX_O>;
			output-enable;
		};

		group2 {
			pinmux = <PI10_LPUART3_RX>;
			input-enable;
		};
	};
};
