/ {
	nrf_radio_coex: coex {
		compatible = "nordic,nrf7002-coex";
		status = "okay";
		status0-gpios = <&gpio0 3 GPIO_ACTIVE_HIGH>;
		req-gpios = <&gpio0 4 GPIO_ACTIVE_HIGH>;
		grant-gpios = <&gpio1 7 (GPIO_PULL_DOWN | GPIO_ACTIVE_LOW)>;
	};
};
