#include <freq.h>

/ {
	chosen {
		zephyr,wifi = &wlan0;
	};
};

&arduino_spi {
	status = "okay";

	nrf70: nrf7002-spi@0 {
		compatible = "nordic,nrf7002-spi";
		status = "okay";
		reg = <0>;
		spi-max-frequency = <DT_FREQ_M(8)>;

		/* Include common nRF70 overlays */
		#include "nrf7002ek_common.dtsi"
		#include "nrf7002ek_common_5g.dtsi"
	};
};
