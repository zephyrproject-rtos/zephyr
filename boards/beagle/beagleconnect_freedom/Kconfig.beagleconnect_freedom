config BOARD_BEAGLECONNECT_FREEDOM
	select SOC_CC1352P7 if BOARD_BEAGLECONNECT_FREEDOM_CC1352P7
	select SOC_CC1352P if BOARD_BEAGLECONNECT_FREEDOM_CC1352P
